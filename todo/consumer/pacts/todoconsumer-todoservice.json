{
  "consumer": {
    "name": "ToDoConsumer"
  },
  "provider": {
    "name": "ToDoService"
  },
  "interactions": [
    {
      "description": "A request to create todo",
      "request": {
        "method": "POST",
        "path": "/v1/todo",
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "completed": true,
          "description": "discuss about OKRs",
          "title": "1-1 with manager"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "id": "string",
          "title": "string"
        },
        "matchingRules": {
          "$.body.id": {
            "match": "type"
          },
          "$.body.title": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}